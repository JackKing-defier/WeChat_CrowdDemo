/**app.wxss**/
page {
  box-sizing: border-box;
  font-size: 30rpx;
  width: 100%;
  height: 100%;
}

view,
input {
  box-sizing: border-box;
}

/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  font-size: 30rpx;
  font-family: "微软雅黑";
  color: #2b2b2b;
}
input{
   font-family: "微软雅黑";
}
